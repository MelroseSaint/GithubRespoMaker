{
  "name": "respogen",
  "version": "1.0.0",
  "description": "All-in-one repository generator for developers - Create professional project structures with templates, CI/CD, Docker, testing, and more",
  "main": "server.js",
  "scripts": {
    "start": "node server.js",
    "dev": "nodemon server.js --watch server.js --watch public/",
    "dev:debug": "nodemon --inspect server.js",
    "test": "echo \"Error: no test specified\" && exit 1",
    "lint": "echo \"Add ESLint configuration for linting\"",
    "format": "echo \"Add Prettier configuration for formatting\"",
    "build": "echo \"Production build - minify assets if needed\"",
    "serve": "npm start",
    "clean": "echo \"Clean temporary files and logs\"",
    "docker:build": "docker build -t respogen .",
    "docker:run": "docker run -p 3000:3000 respogen",
    "docker:dev": "docker-compose -f docker-compose.dev.yml up",
    "health": "curl -f http://localhost:3000/health || exit 1"
  },
  "keywords": [
    "repository-generator",
    "project-templates",
    "developer-tools",
    "code-generator",
    "scaffolding",
    "boilerplate",
    "ci-cd",
    "docker",
    "testing",
    "linting"
  ],
  "author": "RespoGen Contributors",
  "license": "MIT",
  "type": "commonjs",
  "dependencies": {
    "adm-zip": "^0.5.16",
    "archiver": "^7.0.1",
    "cors": "^2.8.5",
    "express": "^5.1.0",
    "mime-types": "^3.0.1",
    "multer": "^2.0.2"
  },
  "devDependencies": {
    "nodemon": "^3.1.10"
  }
}
